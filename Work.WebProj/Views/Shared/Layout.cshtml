    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="@Resources.ResWeb.keywords" />
    <meta name="description" content="@Resources.ResWeb.description" />
    <meta property="og:title" content="@ViewBag.Title" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="@ViewBag.Url" />
    <meta property="og:description" content="@Resources.ResWeb.description" />

    <title>@Resources.ResWeb.WebTitle:@ViewBag.Category:@ViewBag.Title</title>
    @if (ViewBag.IsFirstPage)
    {
        <link href="~/Content/css/index.css" rel="stylesheet" type="text/css"> }
    else
    {
        <link href="~/Content/css/page.css" rel="stylesheet" type="text/css"> }
    @RenderSection("css", false)
